{
    "global": {
        "depends-on": [
            {
                "name": "git",
                "recipe": {}
            },
            {
                "name": "python",
                "recipe": {}
            },
            {
                "name": "brew",
                "recipe": {
                    "darwin": [
                        "ruby -e \"$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)\""
                    ],
                    "linux": []
                }
            },
            {
                "name": "node",
                "recipe": {
                    "darwin": [
                        "brew install node"
                    ],
                    "linux": [
                        "sudo apt-get install -y node npm"
                    ]
                }
            },
            {
                "name": "jsctags",
                "recipe": {
                    "darwin": [
                        "sudo npm install -g git+https://github.com/ramitos/jsctags.git"
                    ],
                    "linux": [
                        "sudo npm install -g git+https://github.com/ramitos/jsctags.git"
                    ]
                }
            },
            {
                "name": "ctags",
                "recipe": {
                    "darwin": [
                        "curl -LSso /usr/local/Library/Formula/ctags-better-php.rb https://raw.githubusercontent.com/shawncplus/phpcomplete.vim/master/misc/ctags-better-php.rb",
                        "brew install ctags-better-php",
                        "brew link --overwrite ctags-better-php"
                    ],
                    "linux": [
                        "curl -LSso ctags-5.8_better_php_parser.tar.gz https://github.com/shawncplus/phpcomplete.vim/raw/master/misc/ctags-5.8_better_php_parser.tar.gz",
                        "tar xvf ctags-5.8_better_php_parser.tar.gz",
                        "./configure",
                        "make",
                        "sudo make install"
                    ]
                }
            }

        ],
        "install-target": "~/.vimpkg/bundle"
    },
    "packages": [
        {
            "depends-on": [],
            "name": "pathogen",
            "pkg-url": "https://github.com/tpope/vim-pathogen.git",
            "recipe": {
                "all": [
                    "mkdir -p ~/.vim/autoload ~/.vim/bundle",
                    "curl -LSso ~/.vim/autoload/pathogen.vim https://tpo.pe/pathogen.vim"
                ]
            }
        },
        {
            "depends-on": [],
            "name": "colors",
